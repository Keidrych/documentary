{
  "name": "documentary",
  "version": "1.26.0",
  "description": "A Documentation Pre-processor To Generate The Table Of Contents, Embed Examples With Their Output, Make Markdown Tables, Maintain Typedefs For JavaScript And README, Watch Changes To Push, Use Macros And Prettify API Titles.",
  "main": "build",
  "scripts": {
    "t": "NODE_DEBUG=doc zoroaster -a",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test": "yarn t test/spec test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "e": "node example",
    "dev": "NODE_DEBUG=doc node src/bin",
    "d1": "typal src",
    "d": "yarn-s d1",
    "doc": "yarn dev -o README.md",
    "start": "node build/bin",
    "example/": "yarn e example/example.js",
    "old-typal": "doc doc/10-typedef -o doc/typal.md",
    "example/toc.js": "NODE_DEBUG=doc yarn e example/toc.js",
    "example/toc2.js": "NODE_DEBUG=doc yarn e example/toc2.js",
    "example/typedef.js": "yarn dev example/typedef.js -T",
    "b": "yarn-s src jsx b2",
    "src": "alamode-depack src -o build -i bin/index.js -s",
    "jsx": "alamode-depack build/components -o build/components -s -j -p",
    "b2": "alamode-depack build/components -o build/components -s",
    "build": "yarn-s b doc",
    "args": "argufy types/arguments.xml -o src/bin/get-args.js"
  },
  "bin": {
    "doc": "build/bin/doc.js",
    "doc-dev": "src/bin/index.js",
    "typal": "build/bin/typal.js"
  },
  "man": "man/doc.1",
  "files": [
    "build",
    "src/bin/index.js",
    "web-components.json"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/documentary.git"
  },
  "keywords": [
    "documentary",
    "documentation",
    "readme",
    "embed",
    "fork",
    "example",
    "examples",
    "output",
    "github",
    "doc",
    "docs",
    "generator",
    "preprocessor",
    "markdown",
    "md",
    "macro",
    "html",
    "jsdoc",
    "typedef",
    "types",
    "methods",
    "api",
    "table of contents",
    "table",
    "toc",
    "heading",
    "jsx",
    "components"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/documentary/issues"
  },
  "homepage": "https://github.com/artdecocode/documentary#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.2.0",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^3.13.0"
  },
  "dependencies": {
    "@depack/cache": "^1.0.0",
    "@depack/render": "^1.1.4",
    "@wrote/clone": "^1.2.1",
    "@wrote/ensure-path": "^1.0.8",
    "@wrote/write": "^1.0.4",
    "alamode": "^2.0.0",
    "argufy": "^1.7.0",
    "catchment": "^3.3.0",
    "clearr": "^1.0.0",
    "competent": "^1.1.0",
    "erte": "^1.1.7",
    "makepromise": "^3.1.0",
    "mismatch": "^1.2.0",
    "pedantry": "^2.3.2",
    "preact": "8.4.2",
    "resolve-dependency": "^1.0.3",
    "restream": "^3.7.1",
    "rexml": "^1.4.0",
    "spawncommand": "^2.2.0",
    "static-analysis": "^1.7.1",
    "typal": "^1.4.0",
    "usually": "^1.2.0",
    "which-stream": "^1.1.0"
  }
}
