{
  "name": "documentary",
  "version": "1.34.10",
  "description": "Documentation Compiler To Generate The Table Of Contents, Embed Examples With Their Output, Make Markdown Tables, Maintain Typedefs For JavaScript And README, Watch Changes To Push, Use Macros And Prettify API Titles.",
  "main": "build",
  "scripts": {
    "t": "DOCUMENTARY_SKIP_USER_COMPONENTS=1 NODE_DEBUG=doc zoroaster -a -t 15000",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test": "yarn t test/spec test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "e": "node example",
    "dev": "node src/bin -d",
    "d1": "typal src",
    "d": "yarn-s d1",
    "doc": "yarn dev -o README.md",
    "wiki": "DOCUMENTARY_MAX_COLUMNS=70 yarn dev wiki -W wiki.git",
    "start": "node build/bin",
    "example/": "yarn e example/example.js",
    "old-typal": "doc doc/10-typedef -o doc/typal.md",
    "competent": "alanode build-competent",
    "stdlib": "depack src/stdlib -o stdlib/index.js -a -c -p -s --source_map_include_content",
    "b": "ALAMODE_ENV=build alamode src -o build -i bin/index.js,stdlib.js -s -j -p -m",
    "build": "yarn-s b doc",
    "up": "yarn-up -e preact -u",
    "args": "argufy types/arguments.xml -o src/bin/get-args.js"
  },
  "bin": {
    "doc": "build/bin/doc.js",
    "doc-dev": "src/bin/index.js",
    "typal": "build/bin/typal.js"
  },
  "man": "man/doc.1",
  "files": [
    "build",
    "src/bin/index.js",
    "web-components.json",
    "stdlib"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/documentary.git"
  },
  "keywords": [
    "documentary",
    "documentation",
    "readme",
    "embed",
    "fork",
    "example",
    "examples",
    "output",
    "github",
    "doc",
    "docs",
    "generator",
    "preprocessor",
    "markdown",
    "md",
    "macro",
    "html",
    "jsdoc",
    "typedef",
    "types",
    "methods",
    "api",
    "table of contents",
    "table",
    "toc",
    "heading",
    "jsx",
    "components"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/documentary/issues"
  },
  "homepage": "https://readme.page",
  "devDependencies": {
    "@depack/cache": "^1.1.0",
    "@depack/detect": "^1.1.0",
    "@depack/render": "^1.3.4",
    "@depack/split": "^1.0.0",
    "@externs/preact": "^1.2.9",
    "@idio/debug": "^1.2.3",
    "@wrote/clone": "^1.2.1",
    "@wrote/ensure-path": "^1.1.0",
    "@wrote/read": "^1.0.4",
    "@wrote/read-dir-structure": "^1.3.0",
    "@wrote/write": "^1.0.4",
    "argufy": "^1.7.5",
    "catchment": "^3.3.0",
    "clearr": "^1.0.0",
    "competent": "^3.7.3",
    "depack": "^1.0.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "forkfeed": "^1.1.1",
    "fpj": "^1.3.0",
    "indicatrix": "^1.4.0",
    "makepromise": "^3.1.0",
    "mismatch": "^1.2.0",
    "pedantry": "^2.5.0",
    "reloquent": "^1.3.2",
    "resolve-dependency": "^1.0.3",
    "restream": "^3.7.4",
    "rexml": "^2.2.2",
    "spawncommand": "^2.2.0",
    "static-analysis": "^2.1.0",
    "temp-context": "^2.1.3",
    "usually": "^1.2.0",
    "which-stream": "^1.2.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "alamode": "^3.3.0",
    "preact": "8.5.3",
    "typal": "^1.24.15"
  }
}
