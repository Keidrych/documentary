{
  "name": "documentary",
  "version": "1.29.0",
  "description": "A Documentation Pre-processor To Generate The Table Of Contents, Embed Examples With Their Output, Make Markdown Tables, Maintain Typedefs For JavaScript And README, Watch Changes To Push, Use Macros And Prettify API Titles.",
  "main": "build",
  "scripts": {
    "t": "NODE_DEBUG=doc zoroaster -a -t 5000",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test": "yarn t test/spec test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "e": "node example",
    "dev": "NODE_DEBUG=doc node src/bin",
    "d1": "typal src",
    "d": "yarn-s d1",
    "doc": "yarn dev -o README.md",
    "wiki": "yarn dev wiki -W ../documentary.wiki",
    "start": "node build/bin",
    "example/": "yarn e example/example.js",
    "old-typal": "doc doc/10-typedef -o doc/typal.md",
    "b": "ALAMODE_ENV=build yarn-s src jsx b2",
    "stdlib": "depack-dev src/stdlib -o stdlib.js -a -c -p -s -O 2018 --source_map_include_content",
    "src": "alamode src -o build -i bin/index.js -s",
    "jsx": "alamode build/components -o build/components -s -j -p",
    "b2": "alamode build/components -o build/components -s",
    "build": "yarn-s b doc",
    "args": "argufy types/arguments.xml -o src/bin/get-args.js"
  },
  "bin": {
    "doc": "build/bin/doc.js",
    "doc-dev": "src/bin/index.js",
    "typal": "build/bin/typal.js"
  },
  "man": "man/doc.1",
  "files": [
    "build",
    "src/bin/index.js",
    "web-components.json",
    "stdlib.js", "stdlib.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/documentary.git"
  },
  "keywords": [
    "documentary",
    "documentation",
    "readme",
    "embed",
    "fork",
    "example",
    "examples",
    "output",
    "github",
    "doc",
    "docs",
    "generator",
    "preprocessor",
    "markdown",
    "md",
    "macro",
    "html",
    "jsdoc",
    "typedef",
    "types",
    "methods",
    "api",
    "table of contents",
    "table",
    "toc",
    "heading",
    "jsx",
    "components"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/documentary/issues"
  },
  "homepage": "https://github.com/artdecocode/documentary#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.2.0",
    "reloquent": "^1.3.2",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha",
    "argufy": "^1.7.1",
    "catchment": "^3.3.0",
    "clearr": "^1.0.0",
    "competent": "^1.7.0",
    "erte": "^1.2.0",
    "forkfeed": "^1.1.1",
    "makepromise": "^3.1.0",
    "mismatch": "^1.2.0",
    "@wrote/write": "^1.0.4"
  },
  "dependencies": {
    "@depack/cache": "^1.0.0",
    "@depack/render": "^1.2.1",
    "@wrote/clone": "^1.2.1",
    "@wrote/ensure-path": "^1.1.0",
    "alamode": "^2.3.6",
    "pedantry": "^2.4.0",
    "preact": "8.4.2",
    "resolve-dependency": "^1.0.3",
    "restream": "^3.7.2",
    "rexml": "^2.0.3",
    "spawncommand": "^2.2.0",
    "static-analysis": "^1.7.1",
    "typal": "^1.18.0",
    "usually": "^1.2.0",
    "which-stream": "^1.1.0"
  }
}
