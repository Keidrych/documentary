{
  "name": "documentary",
  "version": "1.3.0",
  "description": "A library to manage documentation, such as README, usage, man pages and changelog.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "zoroaster -b test/spec",
    "test-build": "BABEL_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "e": "node example",
    "dev": "NODE_DEBUG=doc node src/bin/register",
    "doc": "yarn dev documentation -o README.md",
    "start": "node build/bin",
    "example/": "yarn e example/example.js",
    "example/toc.js": "NODE_DEBUG=doc yarn e example/toc.js",
    "build-src": "babel src --out-dir build --source-maps --ignore src/bin/register.js",
    "build": "yarn-s build-src doc"
  },
  "bin": {
    "doc": "build/bin/index.js"
  },
  "man": "man/doc.1",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/documentary.git"
  },
  "keywords": [
    "documentary"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/documentary/issues"
  },
  "homepage": "https://github.com/artdecocode/documentary#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.49",
    "@babel/register": "7.0.0-beta.49",
    "babel-plugin-transform-rename-import": "2.2.0",
    "eslint": "4.19.1",
    "eslint-config-artdeco": "1.0.0",
    "snapshot-context": "2.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "argufy": "1.1.2",
    "catchment": "2.0.1",
    "restream": "2.0.3",
    "spawncommand": "2.0.1",
    "wrote": "1.4.0"
  }
}
