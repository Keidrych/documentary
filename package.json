{
  "name": "documentary",
  "version": "1.20.1",
  "description": "A Documentation Pre-processor To Generate The Table Of Contents, Embed Examples With Their Output, Make Markdown Tables, Maintain Typedefs For JavaScript And README, Watch Changes To Push, Use Macros And Prettify API Titles.",
  "main": "build",
  "scripts": {
    "t": "NODE_DEBUG=doc zoroaster -a",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test": "yarn t test/spec test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "e": "node example",
    "dev": "NODE_DEBUG=doc node src/bin/alamode",
    "d1": "yarn dev src/bin/run.js -g",
    "d2": "yarn dev src/lib/Documentary.js -g",
    "d": "yarn-s d1 d2",
    "doc": "yarn dev documentary -o README.md",
    "start": "node build/bin",
    "example/": "yarn e example/example.js",
    "example/toc.js": "NODE_DEBUG=doc yarn e example/toc.js",
    "example/toc2.js": "NODE_DEBUG=doc yarn e example/toc2.js",
    "example/typedef.js": "yarn dev example/typedef.js -T",
    "b": "alamode src -o build -i bin/alamode.js",
    "build": "yarn-s b doc"
  },
  "bin": {
    "doc": "build/bin/index.js",
    "doc-dev": "src/bin/alamode.js"
  },
  "man": "man/doc.1",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/documentary.git"
  },
  "keywords": [
    "documentary",
    "documentation",
    "readme",
    "embed",
    "fork",
    "example",
    "examples",
    "output",
    "github",
    "doc",
    "docs",
    "generator",
    "preprocessor",
    "markdown",
    "md",
    "macro",
    "html",
    "jsdoc",
    "typedef",
    "types",
    "methods",
    "api",
    "table of contents",
    "table",
    "toc",
    "heading",
    "vuepress",
    "d3",
    "docusaurus",
    "redoc",
    "docsify",
    "esdoc",
    "docco",
    "documentation.js"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/documentary/issues"
  },
  "homepage": "https://github.com/artdecocode/documentary#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.1",
    "snapshot-context": "2.0.4",
    "temp-context": "2.1.0",
    "yarn-s": "1.1.0",
    "zoroaster": "3.6.6"
  },
  "dependencies": {
    "@wrote/clone": "1.1.0",
    "@wrote/ensure-path": "1.0.5",
    "alamode": "1.7.0",
    "argufy": "1.3.2",
    "catchment": "3.2.2",
    "erte": "1.1.6",
    "mismatch": "1.0.3",
    "pedantry": "2.2.0",
    "preact": "8.4.2",
    "preact-render-to-string": "4.1.0",
    "restream": "3.5.0",
    "rexml": "1.2.0",
    "spawncommand": "2.1.0",
    "typal": "1.2.0",
    "which-stream": "1.0.1"
  }
}
