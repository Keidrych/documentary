{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "doc",
        "DOCUMENTARY_SKIP_USER_COMPONENTS": "1",
        // "ALAMODE_ENV": "test-build",
        "DEBUG": "1",
      },
      "console": "integratedTerminal",
      "args": [
        "test/mask", "test/spec", "-a",
        "-w", "-t", "99999999"
      ],
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App",
      "program": "${workspaceFolder}/src/bin/alamode.js",
      "env": {
        "NODE_DEBUG": "doc"
      },
      "args": [
        "documentary",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For Package",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": ["-o", "README.md", "-w"],
      "cwd": "${workspaceFolder}/../../idiocc/goa",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For cookies",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": ["-o", "README.md", "-n", "_goa", "-T", "types/attributes.xml"],
      "cwd": "${workspaceFolder}/../../idiocc/cookies",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For goa-wiki",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": ["../goa/wiki/Request.md", "-o", "Request.md"],
      "cwd": "${workspaceFolder}/../../idiocc/goa.wiki",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For goa-W",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": ["../goa/wiki", "-W"],
      "cwd": "${workspaceFolder}/../../idiocc/goa.wiki",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For accepts-wiki",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": ["../accepts/wiki/Accepts.md", "-o", "Accepts.md"],
      "cwd": "${workspaceFolder}/../../idiocc/accepts.wiki",
      "console": "integratedTerminal",
      "sourceMaps": false,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For Depack/Doc",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": { "NODE_DEBUG": "doc" },
      "args": [
        "documentary/3-0-commonjs.md",
        // "-o", "README.md",
        "-w",
      ],
      "cwd": "${workspaceFolder}/../../depack/depack",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Doc",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "env": {
        "NODE_DEBUG": "doc"
      },
      "args": [
        "src/bin/doc",
        // "test/fixtures/README-source.md"
        // "src/lib/Documentary.js", "-g"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
